{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        // ACTIVE/SUCCESSFUL STATUS
        "issues.open": "#22863a",
        "testing.iconPassed": "#80FF40",
        "testing.iconQueued": "#00AA00",
        /*
        ADDED/UNTRACKED: For the most part, "UNTRACKED" communicates the same status that "ADDED"
                         communicates, except that "UNTRACKED" resources have been added so recently   that they haven't been tracked by "GIT" yet. use: ["~$> git add ."] to change the status of a resource from "UNTRACKED" to "ADDED"*/

        // STAGED
        "gitDecoration.stageDeletedResourceForeground": "#FF2030", // STAGED
        "gitDecoration.stageModifiedResourceForeground": "#FF5808", // STAGED
        "gitDecoration.addedResourceForeground": "#FF5808", // >=> STAGED
        /*

        |==============|| Source Control Status: "ADDED" ||==============| */
        "minimapGutter.addedBackground": "#10D8B8",
        "editorGutter.addedBackground": "#10D8B8",
        "editorOverviewRuler.addedForeground": "#10D8B8",
        "gitDecoration.untrackedResourceForeground": "#10D8B8", // ADDED/UNTRACKED
