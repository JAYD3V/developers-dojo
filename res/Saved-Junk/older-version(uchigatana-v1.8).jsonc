    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": {
            "foreground": "#A77CFF",
            "bold": false,
            "italic": false,
            "underline": false
        },

        "type": {
            "foreground": "#4C8FFF",
            "bold": true,
            "italic": false,
            "underline": false
        }
    },

    "tokenColors": [
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#00C4A4"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#E50000"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#F50000"
            }
        },

        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#650000"
            }
        },

        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#A77CFF",
                "fontStyle": "bold"
            }
        },

        {
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#F50000",
                "fontStyle": "bold underline"
            }
        },

        {
            "scope": ["source", "text"],
            "settings": {
                "foreground": "#AABCE0"
            }
        },

        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#0C6E9C",
                "fontStyle": ""
            }
        },

        {
            "name": "PUNCTUATION/BRACES",
            "scope": [
                "punctuation",
                "punctuation.definition.string",
                "punctuation.definition.string source.css-ignored-vscode",
                "string punctuation.definition.string",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#4C8FFF",
                "fontStyle": ""
            }
        },

        {
            "name": "STORAGE: PRIMITIVE VAR TYPES, STORAGE-MODIFIERS, PREPROCESSORS",
            "scope": [
                "source keyword", // F.Y.I. Most keywords do not fall in this group
                "source storage",
                "meta.preprocessor punctuation.definition.string",
                "meta.preprocessor punctuation.definition"
            ],
            "settings": {
                "foreground": "#4C8FFF",
                "fontStyle": "bold"
            }
        },

        {
            "name": "KEYWORDS: CONTROL-FLOW, OPERATORS-THAT-ARE-A-WORD, KEYWORD-OTHER (Misc), DOUBLE QUOTATION MARKS AROUND STRINGS",
            "scope": [
                "source keyword",
                "source keyword.other",
                "source keyword.control",
                "source keyword.operator.new",
                "source support.type.property-name",
                "meta.scope.recipe"
            ],
            "settings": {
                "foreground": "#4C8FFF",
                "fontStyle": "italic bold"
            }
        },

        {
            "name": "C-Languages mostly: PUBLIC, PRIVATE, PROTECTED",
            "scope": ["storage.type.modifier.access.control"],
            "settings": {
                "foreground": "#4C8FFF",
                "fontStyle": "bold underline"
            }
        },

        {
            "name": "OPERATORS & ACCESSORS",
            "scope": [
                "constant.character.escape",
                "string.quoted.other.lt-gt",
                "constant.other.placeholder",
                "source keyword.operator",
                "keyword.operator.bitwise.shift",
                "source entity.name.operator",
                "source storage.modifier.reference",
                "source entity.name.function.operator.member",
                "meta.preprocessor punctuation.definition",
                "markup.underline.link",
                "keyword.operator.ternary",
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "entity.other.document.begin",
                "keyword.operator.assignment.compound",
                "punctuation.separator.pointer-access",
                "punctuation.separator.key-value.editorconfig",
                "punctuation.definition.directive"
            ],
            "settings": {
                "foreground": "#FC5A4D",
                "fontStyle": ""
            }
        },

        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.logical",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "punctuation.accessor",
                "punctuation.separator.key-value",
                "punctuation.separator.dot-access",
                "punctuation.separator.colon.case",
                "punctuation.separator.scope-resolution"
