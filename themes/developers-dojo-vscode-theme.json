  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#20C29F",
    "namespace.declaration": {
      "foreground": "#20C29F",
      "bold": true,
      "italic": false,
      "underline": false
    },

    "class": "#20C29F",
    "class.declaration": {
      "foreground": "#20C29F",
      "bold": true
    },

    "type": "#AA52FF",
    "function": "#0CB9FF",
    "method": "#0CB9FF",
    "variable": "#BCC0CC",
    "variable.defaultLibrary": "#64BC5C",
    "class.defaultLibrary": "#64BC5C",
    "namespace.defaultLibrary": "#64BC5C"
  },

  "tokenColors": [
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#64DD58"
      }
    },

    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#FFE333"
      }
    },

    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF3633"
      }
    },

    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF3633CC",
        "fontStyle": "bold underline"
      }
    },

    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#A07CFF"
      }
    },

    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#60C04C",
        "fontStyle": "bold"
      }
    },

    {
      "scope": ["source", "text", "variable.other"],
      "settings": {
        "foreground": "#B8BCCC"
      }
    },

    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#0C6E9C",
        "fontStyle": ""
      }
    },

    {
      "name": "PUNCTUATION/BRACES",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#307AFF",
        "fontStyle": ""
      }
    },

    {
      "name": "STORAGE: PRIMITIVE VAR TYPES, STORAGE-MODIFIERS, PREPROCESSORS",
      "scope": [],
      "settings": {
        "foreground": "#307AFF",
        "fontStyle": "bold"
      }
    },

    {
      "name": "KEYWORDS: CONTROL-FLOW, OPERATORS-THAT-ARE-A-WORD, KEYWORD-OTHER (Misc), DOUBLE QUOTATION MARKS AROUND STRINGS",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "meta.scope.recipe",
        "source support.type.property-name",
        "source punctuation.definition.string",
        "string punctuation.definition.string",
        "meta.preprocessor punctuation.definition",
        "meta.preprocessor punctuation.definition.string",
        "punctuation.definition.string source.css-ignored-vscode"
      ],
      "settings": {
        "foreground": "#307AFF",
        "fontStyle": "italic bold"
      }
    },

    {
      "name": "C-Languages mostly: PUBLIC, PRIVATE, PROTECTED",
      "scope": ["storage.type.modifier.access.control"],
      "settings": {
        "foreground": "#307AFF",
        "fontStyle": "bold underline"
      }
    },

    {
      "name": "OPERATORS & ACCESSORS",
      "scope": [
        "source keyword.operator",
        "keyword.operator.bitwise.shift",
        "source entity.name.operator",
        "source storage.modifier.reference",
        "source entity.name.function.operator.member",
        "meta.preprocessor punctuation.definition",
        "markup.underline.link",
        "punctuation.accessor",
        // DO NOT DELETE the punctuation.separator.* properties & replace with punctuation.separator
