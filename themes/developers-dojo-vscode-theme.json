  "semanticHighlighting": true,
  /* 
**************************************************************************
    "Semantic-Token-Highlighting-Schema-Example":{
        "foreground": "#00000000",
        "italic": false,
        "underline": false,
        "bold": false
    }
  
***************************************************************************/
  "semanticTokenColors": {
    "class:typescript": "#68D064",
    "type:typescript": "#68D064",
    "interface:typescript": "#68D064",
    "namespace:typescript": "#68D064",

    "function.declaration:typescript": {
      "foreground": "#0BBBFF",
      "italic": false,
      "underline": false,
      "bold": true
    },

    "function:typescript": {
      "foreground": "#0BBBFF",
      "italic": false,
      "underline": false,
      "bold": false
    },

    "method.declaration:typescript": {
      "foreground": "#0BBBFF",
      "italic": false,
      "underline": false,
      "bold": true
    },

    "method:typescript": {
      "foreground": "#0BBBFF",
      "italic": false,
      "underline": false,
      "bold": false
    },

    "parameter:typescript": "#E888E0",

    "property.declaration:typescript": {
      "foreground": "#38C8C4",
      "underline": false,
      "italic": true,
      "bold": false
    },

    "property:typescript": {
      "underline": false,
      "italic": false,
      "bold": false
    }
  },

  "tokenColors": [
    /**

********************************************************
 ALL: Programming Languages (w/ c style syntax)
******************************************************* */

    {
      "name": "INVALID", //  Works w/ some Data-format standards (i.e. JSON)
      "scope": "invalid",
      "settings": {
        "foreground": "#FF302DBB",
        "fontStyle": "bold"
      }
    },

    {
      "name": "ALL: Tokens, ",
      "scope": "token.error-token",
      "settings": { "foreground": "#FF302DBB" }
    },

    {
      "name": "ALL: Tokens, Info",
      "scope": "token.info-token",
      "settings": { "foreground": "#66E055AA" }
    },

    {
      "name": "ALL: Tokens, Warning",
      "scope": "token.warn-token",
      "settings": { "foreground": "#FDDD3D99" }
    },

    {
      "name": "ALL: Tokens, ",
      "scope": "token.debug-token",
      "settings": { "foreground": "#9070FFDD" }
    },

    {
      "name": "ALL: Comments (Modern Fwd-slash Syntaxes)",
      "scope": [
        "comment",
        "comment punctuation", // <- This scope and...
        "punctuation.definition.comment" // <- this scope r both required to target all comment declaration operators
      ],

      "settings": {
        "foreground": "#337C33"
      }
    },

    // Hello is this comment to bright?
    {
      "name": "ALL: Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#9099AC" }
    },
    /*




**********************************************************
                   TYPESCRIPT SPECIFIC
*********************************************************/
    {
      "name": "{TS} ~ PROPERTIES",
      "scope": "variable.other.property.ts",
      "settings": { "foreground": "#38C8C4" }
    },

    {
      "name": "{TS} ~ OBJECTS",
      "scope": [
        "variable.other.object.ts",
        "variable.other.constant.object.ts"
      ],
      "settings": { "foreground": "#E888E0" }
    },

    {
      "name": "{TS} ~ THIS ('Lexically Scoped Variable')",
      "scope": "variable.language.this.ts",
      "settings": {
        "foreground": "#E888E0",
        "fontStyle": "italic"
      }
    },

    {
      "name": "{TS} ~ KEYWORDS",
      "scope": [
        "source.ts keyword",
        "keyword.operator.new.ts",
        "keyword.operator.expression.typeof.ts",
        "keyword.operator.expression.instanceof.ts"
      ],
      "settings": {
        "foreground": "#1C82FF",
        "fontStyle": "italic bold"
      }
    },

    {
      "name": "{TS} ~ UNARY & BINARY OPERATORS",
      "scope": "source.ts keyword.operator",
      "settings": {
        "foreground": "#1C82FF",
        "fontStyle": ""
      }
    },

    {
      // The operators in this scope render as latin-punctuation-marks, which makes them hard to see. To increase their visibility, they have been configured to render boldly.
      "name": "{TS} ~ SMALL OPERATORS",
      "scope": [
        "keyword.operator.type.annotation.ts",
        "punctuation.accessor.ts",
        "punctuation.separator.key-value.ts"
      ],

      "settings": {
        "foreground": "#1C82FF",
        "fontStyle": "bold"
      }
    },

    {
      "name": "{TS} ~ KEYWORDS, STORAGE", // i.e. class, function, let, var, etc...
      "scope": "storage.type",
      "settings": {
        "foreground": "#1C82FF",
        "fontStyle": "bold"
      }
    },

    {
      "name": "{TS} ~ PRIMITIVE ANNOTATIONS",
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "entity.name.type.ts"
      ],

      "settings": { "foreground": "#E888E0" }
    },

    {
      "name": "{TS} ~ STRING LITERALS & STRING-TEMPLATE LITERALS",
      "scope": [
        "string.template.ts",
        "string.quoted.double.ts",
        "string.quoted.single.ts",
        "punctuation.definition.string.end.ts",
        "punctuation.definition.string.begin.ts",
        "punctuation.definition.string.template.end.ts"
      ],
      "settings": { "foreground": "#A0A0FF" }
    },

    {
      "name": "{TS} ~ CONSTANTS",
      "scope": "constant",
      "settings": {
        "foreground": "#FF7450",
        "fontStyle": "" // set to none
      }
    },

    {
      "name": "{TS} ~ BOOLEAN LITERALS",
      "scope": [
        "constant.language.boolean.false.ts",
        "constant.language.boolean.true.ts"
      ],
      "settings": {
        "foreground": "#FF7450",
        "fontStyle": "italic"
      }
    },

    {
      "name": "{TS} ~ ESCAPED CHARACTERS",
      "scope": "constant.character.escape.ts",
      "settings": {
        "foreground": "#FF7450",
        "fontStyle": "bold"
      }
    },

    {
      "name": "{TS} ~ UNDEFINED, NULL and NaN",
      "scope": [
        "constant.language.undefined.ts",
        "constant.language.null.ts",
        "constant.language.nan.ts"
      ],
      "settings": {
        "foreground": "#D02050",
        "fontStyle": "italic"
      }
    },

    {
      "name": "{TS} ~ NUMERIC LITERALS (numbers)",
      "scope": "constant.numeric.decimal.ts",
      "settings": {
        "foreground": "#0CC333",
        "fontStyle": ""
      }
    },

    // TODO: Handle 'RegExp' as an embedded language in 'TypeScript'.

    {
      "scope": "keyword.other.ts",
      "name": "UNIQUE/SPECIAL KEYWORDS",
      "settings": {
        "foreground": "#0CC333",
        "fontStyle": "italic"
      }
    },

    {
      "name": "{TS} ~ ESCAPED CHARACTERS",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#0CC333",
        "fontStyle": ""
      }
    },

    /*

*******************************************************
                      JSON & JSONC
*******************************************************     */
    {
      "name": "{JSON} ~ KEYS",
      "scope": "support.type.property-name.json",
      "settings": { "foreground": "#08BEFF" }
    },

    {
      "name": "{JSON} ~ STRINGS",
      "scope": "string.quoted.double.json",
      "settings": { "foreground": "#88BBFF" }
    },

    {
      "name": "{JSON} ~ K/V SEPARATORS",
      "scope": "punctuation.separator.dictionary.key-value.json",
      "settings": {
        "foreground": "#08BEFF",
        "fontStyle": "bold"
      }
    },
    /*
 
    



*********************************************************
                  MARKUP & MARKDOWN
********************************************************/
    {
      "name": "{MARKUP} ~ TEXT/FOREGROUND",
      "scope": ["text"],
      "settings": { "foreground": "#B8BCCC" }
    },

    {
      "name": "{MARKUP} ~ TypeScript: HEADERS",
      "scope": "markup.heading",
      "settings": { "foreground": "#68D064", "fontStyle": "bold" }
    },
    /*


********************************************************
                        JS-DOC
********************************************************/
    {
      "name": "{JSDOC} ~ ANNOTATED DOCUMENTATION CLASSES",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#1C82FF99",
        "fontStyle": "italic bold"
      }
    },

    {
      "name": "{JSDOC} ~ ANNOTATION PUNCTUATION",
      "scope": "punctuation.definition.block.tag.jsdoc",
      "settings": { "foreground": "#FF802088" }
    },
    /*
 
    
********************************************************
                     IGNORE-LISTS
********************************************************/
    {
      "name": "{IGNORE} ~ TEXT",
      "scope": "source.ignore",
      "settings": { "foreground": "#38C8C4" }
    },
    {
      "name": "{IGNORE} ~ COMMENTS",
      "scope": ["comment.line.number-sign"],
      "settings": {
        "foreground": "#1C82FF",
        "fontStyle": "bold underline"
      }
    }
  ]
}
